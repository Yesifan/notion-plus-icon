{
  "name": "notion-plus-icon",
  "manifest_version": 3,
  "version": "1.0.0",
  "description": "use more icon for notion",
  "icons": {
    "48": "icon48.png"
  },
  "permissions": [
    "tabs",
    "webRequest"
  ],
  "host_permissions": ["https://www.notion.so/*"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://www.notion.so/*"],
      "js": ["content.js"]
    }
  ]
}